{
  "name": "basic-ftp-ct",
  "version": "4.5.5",
  "description": "FTP client for Node.js, supports explicit FTPS over TLS, IPv6, Async/Await, and Typescript.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run lint && tsc && mocha",
    "test": "npm run prepublishOnly",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.ts\"",
    "dev": "npm run clean && tsc --watch",
    "tdd": "mocha --watch",
    "buildOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clevertech/basic-ftp.git"
  },
  "author": "Patrick Juchli <patrickjuchli@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ftp",
    "ftps",
    "promise",
    "async",
    "await",
    "tls",
    "ipv6",
    "typescript"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "13.9.2",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "eslint": "^6.8.0",
    "js-yaml": ">=3.13.1",
    "minimist": ">=1.2.2",
    "mocha": "7.1.1",
    "mock-fs": "4.11.0",
    "rimraf": "3.0.2",
    "typescript": "3.8.3"
  }
}
